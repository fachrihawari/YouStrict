{
  "expo": {
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/images/icon-400x240.png"
        }
      ],
      "expo-router",
      "expo-audio",
      "expo-sqlite",
      "expo-asset",
      "expo-secure-store",
       [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/images/icon-1920x720.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "icon": "./assets/images/icon-1024x1024.png",
      "splash": {
        "image": "./assets/images/icon-1920x720.png"
      },
      "package": "com.fachrihawari.YouStrict",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "newArchEnabled": true,
    "name": "YouStrict",
    "scheme": "youstrict",
    "slug": "YouStrict",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "66ba3101-5508-4afb-aad6-2274a5f08683"
      }
    },
    "owner": "hawari"
  }
}
